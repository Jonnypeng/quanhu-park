body {
background: #eee;
/*margin: 50px auto;*/
padding: 0;
}


#wrapper {
width: 960px;
margin: 0 auto;
}

#wrapper {
background:url(../img/kv-img/bg.jpg) no-repeat center center;
background-size:100% 100%;
top:0px;
perspective: 0;
-webkit-perspective:400;
width: 100%;
height:100%;
position:fixed;
	 perspective-origin: -50% 50%;
	 -webkit-perspective-origin: -50% 50%;
transition: perspective, 1s;
	    -o-transition: -o-perspective, 1s;
	    -moz-transition: -moz-perspective, 1s;
	    -webkit-transition: -webkit-perspective, 1s;
overflow:hidden;
}

/*#image:hover {
  animation-play-state:paused;
  -o-animation-play-state:paused;
  -moz-animation-play-state:paused;
  -webkit-animation-play-state:paused;
  }*/


@-webkit-keyframes spin {
	from {
transform: rotateY(0);
	   -o-transform: rotateY(0);
	   -ms-transform: rotateY(0);
	   -moz-transform: rotateY(0);
	   -webkit-transform: rotateY(0);
	}
	to {
transform: rotateY(-360deg);
	   -o-transform: rotateY(-360deg);
	   -ms-transform: rotateY(-360deg);
	   -moz-transform: rotateY(-360deg);
	   -webkit-transform: rotateY(-360deg);
	}
}

#image {
    margin: 0 auto;
    /* height: 300px; */
    transform-style: preserve-3d;
    -webkit-transform-style: preserve-3d;
   /* animation: spin 15s infinite linear ;
    -moz-animation: spin 15s infinite linear ;
    -o-animation: spin 15s infinite linear ;
    -webkit-animation: spin 15s infinite linear ;*/
    position: absolute;
    top: 50%;
}

.image {
position: absolute;
height: auto;
width: 102%;
}

#image > .i1 {
position: absolute;
    transform-style: preserve-3d;
    width: 129px;
    height: 1170px;
    background-image: url(../img/kv-img/bg-team/背景1_01.png);
    backface-visibility: hidden;
    transform-origin: 50% 50% 0px;
    opacity: 1;
    display: block;
}
